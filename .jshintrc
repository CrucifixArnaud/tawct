{
    "node": true,
    "browser": true,
    "esnext": true,
    "bitwise": true,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "immed": true,
    "indent": 4,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "quotmark": "single",
    "undef": true,
    "unused": true,
    "strict": false,
    "trailing": true,
    "smarttabs": true,
    "expr": false,
    "unused": false,
    "globals": {
        "setStyle": true,
        "hasClass": true,
        "requestAnimationFrame": true,
        "cleanWhitespace": true,
        "removeClass": true,
        "addClass": true,
        "BrowserDetect": true,
        "colorToHex": true
    }
}
